{
  "name": "vite-i18n-react-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "build:test": "tsc && vite build --mode test",
    "build:pro": "tsc && vite build --mode production",
    "lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
    "prettier:style": "stylelint --cache --fix \"**/*.{less,postcss,css}\" --cache --cache-location node_modules/.cache/stylelint/",
    "prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,html,md}\"",
    "translate": "ts-node ./scripts/translate.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "antd": "^5.1.1",
    "antd-mobile": "^5.27.0",
    "dayjs": "^1.11.7",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-intl": "^6.2.5",
    "react-redux": "^8.0.5",
    "react-router": "^6.6.1",
    "react-router-dom": "^6.6.1",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-react": "^3.0.0",
    "cssnano": "^5.1.14",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "language-translate": "^1.1.5",
    "less": "^4.1.3",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "stylelint": "14.6.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "25.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.12.1",
    "vite": "^4.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-svg-icons": "^2.0.1"
  }
}
